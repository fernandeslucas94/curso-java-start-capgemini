package conceitoPolimorfismo;

public class Carro {
	String cor, marca, modelo;
	int anoFabricacao, anoModelo;
	
	String mostrarDadosCarro(String cor, String marca, String modelo, int anoFabricacao, int anoModelo) {
		return "A cor do carro é " + cor + ", a marca é " + marca + ", e seu modelo é " + modelo + ". O ano de fabricação é " + anoFabricacao + ", e o ano do modelo é: " + anoModelo;
	}
}
