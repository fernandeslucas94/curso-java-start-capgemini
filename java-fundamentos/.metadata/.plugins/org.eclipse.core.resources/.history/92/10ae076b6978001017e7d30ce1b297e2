package conceitoPolimorfismo;

public class Principal {
	public static void main(String[] args) {
		Carro carro1 = new BMW();
		
		carro1.cor = "Branca";
		carro1.marca = "BMW";
		carro1.modelo = "x4";
		carro1.anoFabricacao = 2025;
		carro1.anoModelo = 2025;
		
		System.out.println(carro1.mostrarDadosCarro(carro1.cor, carro1.marca, carro1.modelo, carro1.anoFabricacao, carro1.anoModelo));
	}
}
