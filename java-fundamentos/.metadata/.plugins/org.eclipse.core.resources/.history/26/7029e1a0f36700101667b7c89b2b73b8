package classeConceitos;

public class PrincipalClasses {
	public static void main(String[] args) {
		Classe carro1 = new Classe();
		
		carro1.nomeCarro = "Uno";
		carro1.modeloCarro = "Mille";
		carro1.marcaCarro = "Fiat";
		carro1.corCarro = "Branco";
		carro1.anoFabricacao = 2005;
		carro1.anoModelo = 2005;
		
		System.out.println(carro1.mostrarDados(carro1.nomeCarro, carro1.modeloCarro, carro1.marcaCarro, carro1.corCarro, carro1.anoFabricacao, carro1.anoModelo));
	}
}
