package conceitoHeranca;

public class Vendedor extends Funcionario {
	public int quantidadeVendasMes, salario;
	public float comissao;
	
	public String calcularSalario(int salario) {
		float salarioVendedor = salario * comissao;
		return "A comissão do vendedor é de" + comissao + "e seu salário é de: R$ " + salarioVendedor + "!";
	}
}
